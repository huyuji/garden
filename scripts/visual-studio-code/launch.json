{
    "configurations": [
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000,
            "stopOnEntry": true,
            "pathMappings": {
            },
        },
        {
            "name": "report42 go server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/go/src/src.circlecvi.com/report42server",
            "env": {},
            "args": [
                "--production=false",
                "--loglevel=debug",
                "--stdout=true"
            ],
            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 1000,
                "maxArrayValues": 1000,
                "maxStructFields": -1
            }
        }
    ]
}
